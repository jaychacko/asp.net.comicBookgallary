@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Comic Book Summary";
    
}


        <h1>@ViewBag.seriesTitle</h1>
        <h2># @ViewBag.issueNumber</h2>
        <h5>Description</h5>
        <div>@Html.Raw(ViewBag.description)</div>

        @if(ViewBag.artists.Length >0){
        <h5>Artists</h5>
        <div>
            <ul>
                @foreach (var a in ViewBag.artists)
                {
                    <li>@a</li>
                }
                
            </ul>
        </div>
        }
 